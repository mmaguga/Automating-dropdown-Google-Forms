function onFormSubmit() {
  var form = FormApp.getActiveForm(); // get the active Google Form
  var item = form.getItemById('ITEM_ID'); // replace ITEM_ID with the ID of the dropdown question
  var itemType = item.getType(); // get the type of the form item
  if (itemType === FormApp.ItemType.MULTIPLE_CHOICE) { // check if the form item is a multiple choice question
    var choices = item.asMultipleChoiceItem().getChoices(); // get the existing choices for the question
    var ss = SpreadsheetApp.openById('SPREADSHEET_ID'); // replace SPREADSHEET_ID with the ID of the Google Spreadsheet
    var sheet = ss.getSheetByName('SHEET_NAME'); // replace SHEET_NAME with the name of the sheet in the spreadsheet that contains the dropdown options
    var agents = sheet.getRange('A2:A').getValues().filter(String); // get the list of agents from column A, excluding any empty cells
    var newChoices = [];
    for (var i = 0; i < agents.length; i++) {
      var agent = agents[i][0];
      if (!choices.some(function(choice) { return choice.getValue() === agent }) && !newChoices.some(function(choice) { return choice.getValue() === agent })) {
        newChoices.push(item.asMultipleChoiceItem().createChoice(agent)); // create a new choice for any agent not already in the dropdown list
      }
    }
    if (newChoices.length > 0) {
      item.asMultipleChoiceItem().setChoices(choices.concat(newChoices)); // add the new choices to the existing choices for the question
    }
  }
